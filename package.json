  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-events-calendar/mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/the-events-calendar/mcp-server/issues"
  },
  "homepage": "https://github.com/the-events-calendar/mcp-server#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1",
    "dotenv": "^17.2.0",
    "undici": "^7.11.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
